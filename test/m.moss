sub counter(a,b)
  i=a
  return sub||
    if i>b
      return empty
    else
      i=i+1
      return i-1
    end
  end
end

sub counter(a,b)
  return sub*||
    for x in a..b
      yield x
    end
  end
end

c = counter(1,4)
print(c.list())



