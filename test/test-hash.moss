
use data: hash
use sys: exit

function error(s)
   print("Error: hash test ", s, " failed.")
   exit(1)
end

h = |s| hex(hash(s.encode("UTF-8")))

s = ""
for k in 1000000
   s = h(s)
end

if s!="4cf39fb768adc6a711513271df57c3a553972e7c75e8a861ff9d820276c90ec9"
   error("#1")
end

